<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView14"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="366dp"
        android:src="@drawable/home1"
        android:scaleType="fitStart" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton13"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="115dp" />

    <FrameLayout
        android:layout_width="90dp"
        android:layout_height="220dp"
        android:layout_marginLeft="0dp"
        android:id="@+id/frameLayout2"
        android:layout_centerVertical="true"
        android:layout_toStartOf="@+id/imageButton13">

    </FrameLayout>

    <ImageButton
        android:layout_width="100dp"
        android:layout_height="130dp"
        android:id="@+id/imageButton14"
        android:background="@drawable/studyr"
        android:layout_marginLeft="60dp"
        android:layout_alignParentBottom="true"
        android:layout_toStartOf="@+id/frameLayout2"
        android:layout_marginBottom="129dp" />

    <ImageButton
        android:layout_width="100dp"
        android:layout_height="110dp"
        android:id="@+id/imageButton15"
        android:background="@drawable/testr"
        android:layout_marginRight="60dp"
        android:layout_alignTop="@+id/imageButton14"
        android:layout_toEndOf="@+id/frameLayout2" />

</RelativeLayout>
